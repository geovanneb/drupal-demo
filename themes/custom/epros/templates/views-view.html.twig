{% elseif id == '_epros_taxonomy_term' %}
	{% if display_id == 'block_on_sidebar_blog_tags' %}
		<div class="widget-content tag-cloud">
			{% set len = rows.0['#rows']|length %}
			{% set i = 1 %}
            {% for row in rows.0['#rows'] %}
            	{% if i != len %}
            		{{ row }},
            		{% set i = i + 1 %}
            	{% else %}
            		{{ row }}
            	{% endif %}
            {% endfor %}
        </div>
    {% elseif display_id == 'block_on_sidebar_blog_categories' %}
    	<div class="widget-content">
		    <ul class="epros-categories-blogs-sidebar">
		        {{ rows }}
		    </ul>
		</div>
	{% elseif display_id == 'block_on_sidebar_product_categories' %}
		<div class="widget-content">
		    <ul class="epros-categories-product-sidebar">
		        {{ rows }}
		    </ul>
		</div>
	{% elseif display_id == 'block_footer_8_column_2_popular_tags' %}
		<div class="tag-cloud">   
		    {{ rows }} 
		</div> 	
	{% else %}
		{{ header }}
	  	{{ rows }}
	  	{{ attachment_after }}
	  	{{ empty }}
	  	{{ footer }}
	  	{{ pager }}
	{% endif %}
{% elseif id == '_epros_shop_catalog' %}
	<div class="row space-bottom-50">
	    <div class="col-md-6 col-sm-5 col-xs-3 show-result"> 
		    {% if rows.0['#rows']|length == 1 %}
		    	<h2 class="section-title size-18">Showing {{ rows.0['#rows']|length }} product</h2>
		    {% else %}
		    	<h2 class="section-title size-18">Showing {{ rows.0['#rows']|length }} products</h2>
	    	{% endif %}
	     </div>
	    <div class="col-md-4 col-sm-4 col-xs-5 sorter epros-exposed"> 
	        {{ exposed }}
	    </div>
	</div>

	<div class="tab-content">
		{% if shop_listing_layout == 'grid' %}
	    <div id="grid-view" class="tab-pane fade active in" role="tabpanel">
	        <div class="row text-center">
	            {{ rows }}
	        </div>
	        <div class="pagination-wrapper space-50">
	            {{ pager }}
	        </div>
	    </div>
	    {% else %}
	    <div id="list-view" class="tab-pane fade active in" role="tabpanel">
	        {{ rows }}

	        <div class="pagination-wrapper space-50">
	            {{ pager }}
	        </div>
	    </div>
	    {% endif %}
	</div>

	<div class="visible-lg space-top-40"></div>
{% else %}
	{{ header }}
  	{{ rows }}
  	{{ attachment_after }}
  	{{ empty }}
  	{{ footer }}
  	{{ pager }}
{% endif %}