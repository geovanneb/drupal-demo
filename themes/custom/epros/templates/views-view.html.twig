{% if id == '_epros_sidebar_widgets' %}
	{% if display_id == 'block_sidebar_recent_products' %}
	    <div class="widget-content epros-recent-products">
	        {{ rows }}                                            
	    </div>
	{% elseif display_id == 'block_sidebar_product_recent_products' %}
				{{ header}}
		<div class="recent-nav">                
		    <span class="prev slider-btn arrow_carrot-left"> </span>
		    <span class="next slider-btn arrow_carrot-right"> </span>
		</div>
		<div class="recent-prod-slider space-bottom-30">
		    <!-- Slides -->
		    {% set temp = 1 %}
		    {% for i, row in rows.0['#rows'] %}
		    	{% if temp == 1 %}
			    	<div class="item">
			        	{{ row }}
			        {% set temp = temp + 1 %}
			    {% elseif temp == 2 %}
			    	{{ row }}
			    	{% set temp = temp + 1 %}
			    {% elseif temp == 3 or i == (rows.0['#rows']|length - 1) %}
			    		{{ row }}
			    	</div>
			    	{% set temp = 1 %}
			    {% endif %}
		    {% endfor %}
		</div>
	{% elseif display_id == 'block_sidebar_product_shop_recent_products' %}
		<div class="widget-content">
		    {{ rows }}                                                                   
		</div>	
	{% else %}
		{{ header }}
	  	{{ rows }}
	  	{{ attachment_after }}
	  	{{ empty }}
	  	{{ footer }}
	  	{{ pager }}
	{% endif %}
{% elseif id == '_epros_section_content' %}
