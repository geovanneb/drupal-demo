		    <div class="featured-slider text-center space-top-40 slide-nav-1">
		    	{% set tempt = 1 %}
			    {% for i, row in rows.0['#rows'] %}
			    	{% if tempt == 1 %}
				    	<div class="item">
				        	{{ row }}
				        {% set tempt = tempt + 1 %}
				    {% elseif tempt == 2 or i == (rows.0['#rows']|length - 1) %}
				    		{{ row }}
				    	</div>
				    	{% set tempt = 1 %}
				    {% endif %}
			    {% endfor %}
